<?php

/**
 * @file
 * Install/update hooks.
 */

declare(strict_types=1);

/**
 * Set the new index_previous_revisions setting.
 */
function search_api_revisions_update_10001(): void {
  \Drupal::configFactory()->getEditable('search_api_revisions.settings')
    ->set('index_previous_revisions', TRUE)
    ->save();
}
